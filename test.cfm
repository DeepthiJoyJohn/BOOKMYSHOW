				};

				BMS.Misc.fnPushEventDataToAnalytics(["CC"], "", dataObjCC);

				var genreList = "".split("|");

				genreList.forEach(function(item){
					BMS.Misc.fnPushEventDataToAnalytics(["CC"], "", { "Movie Genre": item });
				});

			</script>
