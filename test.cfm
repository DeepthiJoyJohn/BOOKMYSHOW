		<!-- Analytics for cinema  -->
		
		<script type="text/javascript">

			var displayed_popup = null;
			var deferred = {
				seatlayoutjs: '',
				seatlayouthtml: '',
				getSynopsysData:'',
			};

			jQuery(document).ready(function($) {

		 		//Search Hide and Show on Click
		        $(".__search").on("click",function(e){

					$(this).addClass("_active");
					$("#fltrsearch").focus();
					e.stopPropagation();
				});
			    $("#fltrsearch").blur(function(){
					$(".__search").removeClass("_active");
			    });
				$(".icon-close").on("click",function(e){

					$(this).parent().removeClass("_active");
					$("#fltrsearch").val("");
					$("#fltrsearch").blur();
					$("#fltrsearch").trigger("keyup"); //reset search and render
					e.stopPropagation();
				});

				$.ajaxSetup({ cache: true });
			    deferred.seatlayoutjs =  $.getScript(getSeatScript, function(data, textStatus, jqxhr) {},true);
			    deferred.seatlayouthtml = $.ajax({type: "GET",  url:    "/serv/getData.bms",data:   {cmd: "SEATHTML", eventGroup: (eventGroup ? eventGroup : "")}});


		 	    $.when(deferred.seatlayoutjs, deferred.seatlayouthtml).then(function(jsSeat,htmlSeat) {
					$("body").append(htmlSeat[0]);
					$.ajaxSetup({ cache: false });
				});

		 		/**********************Added condition to check if we are getting here by venue or not. In Case of Venue wedonot need groupratings ************************************/

		 		if (byWhat == "ET") {
			 		deferred.getRatingsData = $.ajax({type: "GET",  url:    "/serv/getData.bms",data:   {cmd: "GETGROUPRATING",event_group:eventGroup}});

					$.when(deferred.getRatingsData).then(function(ratingsData) {
						var data=JSON.parse(ratingsData);
						if (!_.isEmpty(data)) {
							$(".heart-rating").show();
							if (data.UserCount != 0 || data.CriticCount != 0) {
								$(".__votes").html(data.UserCount+data.CriticCount+" VOTES");
							} else {
								$(".__votes").hide();
							}
							if (data.AverageRating!=0) {
								$(".__percent").html(data.avgRatingStr);
								$(".heart-icon").show();
							} else {
								$(".__percent").hide();
								$(".heart-icon").hide();
							}
						} else {
							$(".heart-rating").hide();
						}
					});
				}

			 	/***********************************************************************************************************************************************************************/

				if(!global.blnIsTouchScreen) {
					setTimeout(function() {
						if($('.ad-spot-showtimes #div-gpt-ad-1476078216990-0 div>iframe, .ad-spot-showtimes #div-gpt-ad-1483529389188-0 div>iframe').length) {
							if($('.ad-spot-showtimes #div-gpt-ad-1476078216990-0 div iframe, .ad-spot-showtimes #div-gpt-ad-1483529389188-0 div iframe').width() == "160") {
								$('.showtimes .wrapper .container .ad-spot-showtimes, .showtimes .wrapper .container').addClass('__ab-test');
							}
						}
					}, 2000);
				}

				/* Date changed coachmark begins */

				if(dtToday != dateSelected && $('.tooltip-date-changed').length != 0) {
					if(byWhat == 'VN' && BMS.Storage.get({name : "datechanged", key:"ecCode"}) != Cinema_id) {
						setTimeout(function() {
							$('.movie-details').after('\
								<div class="tooltip-date-changed" style="display: none; position: absolute;top: auto;left: 0px;color: #fff;margin: 0;cursor: default;z-index: 3;"> \
									<span class="__arrow" style="width: 0;height: 0;border-left: 7px solid hsla(0, 0%, 0%, 0);border-right: 7px solid hsla(0, 0%, 0%, 0);position: absolute;top: -7px;left: 14px;border-bottom: 10px solid hsl(222, 30%, 13%);"></span> \
									<span class="__content" style="display: table; background: hsl(222, 30%, 13%);border-radius: 5px;padding: 8px;text-transform: none;font-size: 12px;">Your date has been changed</span> \
								</div>');
							$('.tooltip-date-changed')
								.css('left', $('#showDates li._active').offset().left)
								.css('top', $('#showDates li._active').offset().top + $('.filters-container').outerHeight() + 10);
							$('.tooltip-date-changed').show();
							if ($('.tooltip-date-changed').offset().left + $('.tooltip-date-changed').outerWidth() > $(window).width()) {
								$('.tooltip-date-changed .__arrow').css('display', 'none');
								$('.tooltip-date-changed').css('left', 'auto');
								$('.tooltip-date-changed').css('right', '2px');
							}
						}, 600);
					} else if(byWhat == 'ET' && BMS.Storage.get({name : "datechanged", key:"ecCode"}) != Ecode) {
						$('.movie-details').after('\
							<div class="tooltip-date-changed" style="display: none; position: absolute;top: auto;left: 0px;color: #fff;margin: 0;cursor: default;z-index: 3;"> \
								<span class="__arrow" style="width: 0;height: 0;border-left: 7px solid hsla(0, 0%, 0%, 0);border-right: 7px solid hsla(0, 0%, 0%, 0);position: absolute;top: -7px;left: 14px;border-bottom: 10px solid hsl(222, 30%, 13%);"></span> \
								<span class="__content" style="display: table; background: hsl(222, 30%, 13%);border-radius: 5px;padding: 8px;text-transform: none;font-size: 12px;">Your date has been changed</span> \
							</div>');
						$('.tooltip-date-changed')
							.css('left', $('#showDates li._active').offset().left)
							.css('top', $('#showDates li._active').offset().top + $('.filters-container').outerHeight() + 10);
						$('.tooltip-date-changed').show();
					}
				}

				if(byWhat == 'VN') {
					BMS.Storage.set({ name: "datechanged", value: Cinema_id, key: "ecCode", storage: "S", sess: true });
				} else {
					BMS.Storage.set({ name: "datechanged", value: Ecode, key: "ecCode", storage: "S", sess: true });
				}

				/* Date changed coachmark ends */

				/* Language check coachmark BEGINS */
				if (byWhat == 'ET') {
					if ($('#filterLanguage li').length > 1) {
						$('.movie-details').after('\
							<div class="coachmark-language-chk" style="display: none; position: absolute;top: auto;left: 0px;color: #fff;margin: 0;cursor: default;z-index: 3;"> \
		                  		<span class="__arrow" style="width: 0;height: 0;border-left: 7px solid hsla(0, 0%, 0%, 0);border-right: 7px solid hsla(0, 0%, 0%, 0);position: absolute;top: -7px;left: 155px;border-bottom: 10px solid hsl(222, 30%, 13%);"></span> \
		                 		<span class="__content" style="display: table; background: hsl(222, 30%, 13%);border-radius: 5px;padding: 8px;text-transform: none;font-size: 12px;">Check movie language and format before proceeding</span> \
		  					</div>');
						$('.coachmark-language-chk').css({
							'left' : (($('#filterLanguage').offset().left + $('#filterLanguage').outerWidth() / 2) - ($('.coachmark-language-chk').outerWidth() / 2)),
							'top' : $('.filters-wrapper').offset().top + $('.filters-wrapper').outerHeight() + 25
						});
						$('.coachmark-language-chk').show();
					}
				}
				/* Language check coachmark ENDS */
			});

			/* onload calls for coachmark begin */
			$(window).load(function(){
				if(dtToday != dateSelected) {
					fnCoachmarkAnimation('tooltip-date-changed');
				}
				fnCoachmarkAnimation('coachmark-language-chk');
			});
			/* onload calls for coachmark end */

			/* Coachmark animation */
			function fnCoachmarkAnimation(elemClass) {
				var $coachmark = $('.' + elemClass);
				if ($coachmark.length) {
					$coachmark.animate({
						marginTop: "+=15",
					}, 500, function() {
						$coachmark.animate({
							marginTop:"-=20"
						}, 250, function() {
							$coachmark.animate({
								marginTop:"+=5"
							}, 250);
						});
					});

					setTimeout(function() {
						$coachmark.remove();
					}, 3000);
				}
			}
		</script>

		
					<script type="text/javascript">
				var dataObjCC = {};
				dataObjCC = {
					"Movie language" : "",
